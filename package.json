{
  "name": "vue-devui",
  "version": "0.0.0",
  "license": "MIT",
  "description": "DevUI components based on Vite and Vue3",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "jest --config jest.config.js",
    "lint": "eslint \"{src,devui}/**/*.{vue,js,ts,jsx,tsx}\"",
    "lint:fix": "eslint --fix \"{src,devui}/**/*.{vue,js,ts,jsx,tsx}\"",
    "convert:route": "node ./scripts/convert-component-route.js"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.4",
    "lodash-es": "^4.17.20",
    "vue": "^3.1.1",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vitejs/plugin-vue-jsx": "^1.1.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.6",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "babel-jest": "^27.0.2",
    "commander": "^7.1.0",
    "eslint": "^7.28.0",
    "eslint-plugin-vue": "^7.11.1",
    "husky": "^4.3.7",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "sass": "^1.32.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.1.3",
    "vite": "^2.0.5",
    "vite-plugin-md": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,devui}/**/*.{js,ts,jsx,tsx,vue}": "eslint --fix"
  }
}
